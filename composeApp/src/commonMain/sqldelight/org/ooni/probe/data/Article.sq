CREATE TABLE Article(
    url TEXT PRIMARY KEY,
    title TEXT NOT NULL,
    source TEXT NOT NULL,
    description TEXT,
    time INTEGER NOT NULL
);

insertOrReplace:
INSERT OR REPLACE INTO Article (
    url,
    title,
    description,
    source,
    time
) VALUES (?,?,?,?,?);

selectAll:
SELECT * FROM Article ORDER BY time DESC;

deleteExceptUrls:
DELETE FROM Article WHERE Article.url NOT IN ?;

